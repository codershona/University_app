<div class="row">

	<div class="col s12">
   <h3 class="center-align">SIGN UP HERE!</h3>
   
   </div>



   <%= form_for @student do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
   
   <%= f.label :email %>
   <%= f.email_field :email %>


   <%= f.submit "SUBMIT HERE"  %>

   <% end %>
